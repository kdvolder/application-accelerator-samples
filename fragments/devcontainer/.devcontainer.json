{
    "name": "tanzu-devcontainer",
    "image":"projects.registry.vmware.com/tanzu-developer-containers/tanzu-devcontainer-java:latest",
    "features": {
        "ghcr.io/devcontainers/features/java:1": {
            "version": "17" // <-- based on javaVersion selected in accelerator
            ,"installGradle": true // <-- based on buildType and wrapper choices in accelerator
            ,"installMaven": true // <-- based on buildType and wrapper choices in accelerator
        },
        "ghcr.io/devcontainers/features/docker-outside-of-docker:1": {},
        "ghcr.io/dhoeric/features/google-cloud-cli:1": {
            "INSTALL_GKEGCLOUDAUTH_PLUGIN": "true"
        }
    },
    "customizations": {
        "vscode": {
            "settings": {},
            // Add the IDs of extensions you want installed when the container is created.
            "extensions": [
                "vscjava.vscode-java-pack",
                "vmware.tanzu-dev-tools"
            ]
        }
    },
    "remoteUser": "root"
}